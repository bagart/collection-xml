{
    "name": "mtownsend/collection-xml",
    "description": "The missing XML support for Laravel's Collection class.",
    "keywords": [
        "collection",
        "laravel",
        "xml",
        "soap",
        "toXml",
        "toSoapXml"
    ],
    "authors": [
        {
            "name": "Mark Townsend",
            "email": "mtownsend5512@gmail.com",
            "role": "Developer"
        }
    ],
	"autoload": {
        "psr-4": {
            "Mtownsend\\CollectionXml\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "~7.0",
        "illuminate/support": "^5.3",
        "spatie/array-to-xml": "^2.6"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Mtownsend\\CollectionXml\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mtownsend\\CollectionXml\\Providers\\CollectionXmlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
